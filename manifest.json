{
  "name": "Universal Cookbook",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Simple online conversion between U.S. cups, imperial and metric.",
  "background": {
    "scripts": ["background.js"]
  },
  "icons": { "128": "scales_128x128.png" },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Translate this recipe",
    "default_icon": "scales_16x16.png"
  }
}
